{
  "name": "redis-node-js-cloud",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 80 src/frontend ",
    "start": "cross-env NODE_ENV=production next start -p 80 src/frontend ",
    "build": "cross-env NODE_ENV=production next build src/frontend",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "lint": "eslint",
    "clean": "rm -rf src/frontend/.next",
    "dev-server": "nodemon --config ./nodemon.json",
    "standalone-graphql": "npx tsx --tsconfig src/Server/tsconfig.custom.json src/Server/index.ts",
    "ssl": "npx tsx --tsconfig src/Server/tsconfig.custom.json src/Server/sslServer.ts",
    "generate-schema": "npx @gqty/cli http://localhost:80/api/graphql --target=src/frontend/gqty/index.ts ",
    "setupSecrets": "npx auth@latest secret next"
  },
  "gqty": {
    "introspections": {
      "https://localhost/api/graphql": {}
    }
  },
  "dependencies": {
    "@ant-design/compatible": "^5.1.4",
    "@ant-design/cssinjs": "^1.23.0",
    "@ant-design/icons": "^5.6.0",
    "@ant-design/icons-svg": "^4.4.2",
    "@ant-design/nextjs-registry": "^1.0.2",
    "@graphql-tools/schema": "^10.0.16",
    "@types/bcrypt": "^5.0.2",
    "@types/draftjs-to-html": "^0.8.4",
    "@types/formidable": "^3.4.5",
    "@types/glidejs__glide": "^3.6.6",
    "@types/html-to-draftjs": "^1.5.0",
    "@types/micro-cors": "^0.1.5",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.8",
    "@vercel/blob": "^0.27.1",
    "antd": "^5.23.3",
    "apollo-server": "^3.13.0",
    "apollo-server-micro": "^3.13.0",
    "bcrypt": "^5.1.1",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "cra-template-typescript": "1.2.0",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.1",
    "crypto-js": "^4.2.0",
    "draft-js": "^0.11.7",
    "draft-js-export-html": "^1.4.1",
    "draft-js-import-html": "^1.4.1",
    "draftjs-to-html": "^0.9.1",
    "express": "^4.21.2",
    "express-graphql": "^0.12.0",
    "express-http-proxy": "^2.1.1",
    "formidable": "v3",
    "gqty": "^3.2.2",
    "graphql": "^16.10.0",
    "graphql-http": "^1.22.4",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-tools": "^9.0.11",
    "html-to-draftjs": "^1.5.0",
    "micro": "^10.0.1",
    "micro-cors": "^0.1.1",
    "mongodb": "^6.12.0",
    "net": "^1.0.2",
    "next": "^15.1.6",
    "next-auth": "^4.24.11",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.4",
    "next-transpile-modules": "^10.0.1",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "rc-util": "^5.44.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-drag-reorder": "^1.2.0",
    "react-glidejs": "^1.0.8",
    "react-icons": "^5.4.0",
    "react-scripts": "5.0.1",
    "redis": "^4.7.0",
    "sass": "^1.83.4",
    "styled-components": "^6.1.14",
    "styled-icons": "^10.47.1",
    "tls": "^0.0.1",
    "uppload": "^3.2.1",
    "uppload-react": "^1.2.1",
    "url": "^0.11.4",
    "util": "^0.12.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browser": {
    "crypto": false,
    "stream": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/react-dom": "^19.0.3",
    "@types/react-draft-wysiwyg": "^1.13.8",
    "immutable": "^5.0.3",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
